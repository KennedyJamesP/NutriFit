<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fitness.cs115.a115fitnessapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".meal_addFood"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_viewAllMeals"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_createMeal"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_editMeal"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_barCodeScannerTest"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".meal_overview"
            android:configChanges="orientation"
            android:label="Meal Log"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_programList"
            android:configChanges="orientation"
            android:label="Fitness Log"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_workoutList"
            android:configChanges="orientation"
            android:label="Program Name Placeholder"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_exerciseList"
            android:configChanges="orientation"
            android:label="Program Name Placeholder"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_createWorkout"
            android:configChanges="orientation"
            android:label="New Workout"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:name=".work_createProgram"
            android:label="New Program"
            android:theme="@style/AppTheme" />

        <activity
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:name=".work_tracker"
            android:label="Workout Tracker"
            android:theme="@style/AppTheme" />


        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


    </application>

</manifest>