<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fitness.cs115.a115fitnessapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".meal_addFood"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_SimpleScannerActivity"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_viewAllMeals"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_createMeal"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_editMeal"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_barCodeWebQuerry"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".meal_overview"
            android:configChanges="orientation"
            android:label="Meal Log"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_programList"
            android:launchMode="singleTop"
            android:configChanges="orientation"
            android:label="My Workout Programs"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_workoutList"
            android:launchMode="singleTop"
            android:configChanges="orientation"
            android:label="Program Name Placeholder"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_exerciseList"
            android:launchMode="singleTop"
            android:configChanges="orientation"
            android:label="Program Name Placeholder"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_createWorkout"
            android:launchMode="singleTop"
            android:parentActivityName=".work_createProgram"
            android:configChanges="orientation"
            android:label="Edit Workout"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_Onboarding"
            android:configChanges="orientation"
            android:label="Nutrition Overview"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_createProgram"
            android:launchMode="singleTop"
            android:parentActivityName=".work_programList"
            android:configChanges="orientation"
            android:label="Edit Workout Program"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_tracker"
            android:launchMode="singleTop"
            android:parentActivityName=".work_workoutList"
            android:configChanges="orientation"
            android:label="Workout Tracker"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_viewAllEatenMeals"
            android:configChanges="orientation"
            android:label="Meals Eaten"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_graphs"
            android:label="Meals Eaten"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".meal_viewEatenMeal"
            android:configChanges="orientation"
            android:label="Meal Overview"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".CalendarViewer"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".meal_createDayLog"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_newProgramName"
            android:configChanges="orientation"
            android:label="Enter New Program Name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_newWorkoutName"
            android:configChanges="orientation"
            android:label="Enter New Workout Name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".work_createExercise"
            android:parentActivityName=".work_createWorkout"
            android:launchMode="singleTop"
            android:configChanges="orientation"
            android:label="Edit Exercise"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />

        <activity
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:name=".work_trackerSetList"
            android:parentActivityName=".work_tracker"
            android:launchMode="singleTop"
            android:label="Workout Tracker"
            android:theme="@style/AppTheme" />

        <activity
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:name=".work_trackerSetDetail"
            android:label="Workout Tracker"
            android:theme="@style/AppTheme" />

        <activity
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:name=".graph_maxExerciseWeight"
            android:label="Workout Progress"
            android:theme="@style/AppTheme" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


    </application>

</manifest>